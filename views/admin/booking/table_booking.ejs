<!-- DataTales Example -->
<div class="card shadow mb-4">
	<div class="card-header py-3">
		<h6 class="m-0 font-weight-bold text-primary">
			Data Booking
		</h6>
	</div>
	<div class="card-body">
		<div class="table-responsive">
			<table
				class="table table-bordered"
				id="dataTable"
				width="100%"
				cellspacing="0"
			>
				<thead>
					<tr>
						<th>No</th>
						<th>Date</th>
						<th>Title</th>
						<th>Name Bank</th>
						<th>Name Member</th>
						<th>Status</th>
						<th>Action</th>
					</tr>
				</thead>
				<% for( let i = 0; i < booking.length; i++ ) { %>
				<tbody>
					<tr>
						<td><%= i + 1 %></td>
						<td>
							<%= booking[i].bookingStartDate.toLocaleString().split(',')[0] %>
							s-d <%= booking[i].bookingEndDate.toLocaleString().split(',')[0]
							%>
						</td>
						<td><%= booking[i].itemId.title %></td>
						<td><%= booking[i].bankId.nameBank %></td>
						<td><%= booking[i].memberId.firstName %></td>
						<td><%= booking[i].payments.status %></td>
						<td>
							<a
								href="/admin/booking/<%= booking[i].id %> "
								type="button"
								class="btn btn-success btn-circle btn-sm ml-2 button-update"
							>
								<i class="fas fa-eye"></i>
							</a>
						</td>
					</tr>
				</tbody>
				<% } %>
			</table>
		</div>
	</div>
</div>
